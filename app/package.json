{
  "name": "mavensmate-app",
  "version": "0.0.1",
  "main": "main.js",
  "scripts": {
    "dev": "electron ./app",
    "clean": "rm -rf ../dist",
    "clean:osx": "rm -rf ../dist/osx",
    "clean:win": "rm -rf ../dist/win",
    "clean:linux": "rm -rf ../dist/linux",
    "build": "npm run clean && npm run build:osx && npm run build:win",
    "build:osx": "npm run clean:osx && electron-packager ./ \"MavensMate\" --prune --out=../dist/osx --platform=darwin --arch=x64 --version=0.30.1 --icon=../assets/icon.icns",
    "build:win": "npm run clean:win && electron-packager ./ \"MavensMate\" --prune --out=../dist/win --platform=win32 --arch=ia32 --version=0.30.1 --icon=../assets/icon.ico",
    "build:linux": "npm run clean:linux && electron-packager ./ \"MavensMate\" --prune --out=../dist/linux --platform=linux --arch=x64 --version=0.30.1 --icon=../assets/icon.png",
    "pack": "npm run pack:osx && npm run pack:win",
    "pack:osx": "npm run build:osx && electron-builder \"../dist/osx/MavensMate-darwin-x64/MavensMate.app\" --platform=osx --out=\"../dist/osx\" --config=../packager.json",
    "pack:win": "npm run build:win && electron-builder \"../dist/win/MavensMate-win32-ia32\" --platform=win --out=\"../dist/win\" --config=../packager.json",
    "reset": "npm run clean && rm -rf ./node_modules",
    "install3": "npm install",
    "install2": "npm install"
  },
  "dependencies": {
    "mavensmate": "0.0.38-beta8"
  },
  "devDependencies": {
    "electron-rebuild": "latest",
    "electron-prebuilt": "latest"
  }
}
