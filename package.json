{
  "name": "Mavensmate",
  "version": "0.0.11b",
  "author": "avi",
  "scripts": {
    "postinstall": "install-app-deps",
    "dist": "build",
    "dist:win": "build --win --ia32",
    "release": "build",
    "start": "NODE_ENV=dev electron src/main",
    "dev": "NODE_ENV=dev node_modules/.bin/webpack-dev-server",
    "stage:win": "node bin\\build.js",
    "stage": "node bin/build.js",
    "build:win": "set NODE_ENV=production && webpack -p --config webpack.config.prod.js",
    "build": "NODE_ENV=production webpack -p --config webpack.config.prod.js"
  },
  "main": "src/electron/main.js",
  "build": {
    "appId": "com.mavens.mavensmate",
    "asar": false,
    "productName": "MavensMate",
    "linux": {},
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    }
  },
  "devDependencies": {
    "babel-core": "6.14.0",
    "babel-loader": "6.2.5",
    "babel-preset-es2015": "6.1.18",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-0": "6.1.18",
    "babel-runtime": "^6.2.0",
    "css-loader": "^0.25.0",
    "electron": "^3.1.12",
    "electron-builder": "^19.8.0",
    "file-loader": "^0.8.5",
    "fs-extra": "^0.30.0",
    "gulp": "^3.9.1",
    "modclean": "1.2.7",
    "react": "0.14.3",
    "react-addons-update": "^15.3.1",
    "react-dom": "0.14.3",
    "react-redux": "4.0.0",
    "redux": "3.0.4",
    "redux-devtools": "3.0.0-beta-3",
    "redux-devtools-dock-monitor": "1.0.0-beta-3",
    "redux-devtools-log-monitor": "1.0.0-beta-3",
    "redux-thunk": "1.0.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "style-loader": "0.13.0",
    "svg-inline-loader": "^0.6.1",
    "svg-inline-react": "^1.0.2",
    "url-loader": "0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-server": "1.14.0",
    "webpack-node-externals": "^1.3.3"
  },
  "dependencies": {
    "6": "0.0.1",
    "commander": "git+https://github.com/joeferraro/commander.js.git",
    "mavensmate": "^1.0.0-beta.32"
  }
}
